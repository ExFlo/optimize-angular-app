{
  "name": "optimized-angular-app",
  "version": "1.0.0-alpha.0",
  "description": "Optimized Angular application",
  "keywords": "Angular",
  "author": "Marc Laval",
  "scripts": {
    "clean": "rm -rf dist/",
    "vendors": "bash ./scripts/vendors.sh",
    "public": "npm run clean && mkdir dist && cp -a public/* dist/ && npm run vendors",
    "dev": "npm run public && tsc -w -p tsconfig-systemjs.json",
    "systemjs": "npm run public && tsc -p tsconfig-systemjs.json",
    "bundling-app": "npm run systemjs && node ./scripts/app-system-bundler.js",
    "serve": "http-server ./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mlaval/optimized-angular-app.git"
  },
  "license": {
    "type": "MIT",
    "url": "https://raw.githubusercontent.com/mlaval/optimized-angular-app/master/LICENSE"
  },
  "bugs": {
    "url": "https://github.com/mlaval/optimized-angular-app/issues"
  },
  "homepage": "https://github.com/mlaval/optimized-angular-app#readme",
  "dependencies": {
    "@angular/core": "2.0.2",
    "@angular/compiler": "2.0.2",
    "@angular/common": "2.0.2",
    "@angular/forms": "2.0.2",
    "@angular/http": "2.0.2",
    "@angular/platform-browser": "2.0.2",
    "@angular/platform-browser-dynamic": "2.0.2",
    "@angular/platform-server": "2.0.2",
    "@angular/router": "3.0.2",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.7",
    "classlist-polyfill": "1.0.3",
    "core-js": "2.4.1",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "@angular/compiler-cli": "0.6.4",
    "http-server": "^0.9.0",
    "systemjs": "0.19.39",
    "systemjs-builder": "0.15.32",
    "typescript": "2.0.3"
  }
}